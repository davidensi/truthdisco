      "contracts/Reputations.sol": {
        "content": "pragma solidity 0.7.3;\n\ncontract Reputations {\n\n  mapping (address => int256) private _reputations;\n\n  function reputationOf(address user) external view returns (int256) {\n    return _reputations[user];\n  }\n\n  function updateReputation(address user, int256 reputation) public returns (bool) {\n    _reputations[user] = reputation;\n    return true;\n  }\n\n  function testFunction() external view returns (int256) {\n    return 10;\n  }\n\n}\n"
      }
